<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="healthRecordAction" extends="struts-base" namespace="/healthRecordAction">
	
	<!-- 基本信息 -->
	<action name="queryMemberBaseInfo" class="com.gzjky.action.healthRecordAction.QueryMemberBaseInfoAction" method="getBaseInfo">
     	<result name="success">/jsp/health/healthrecord/memberbaseinfo.jsp</result>
	</action>
	
	<!-- 基本信息编辑 -->
	<action name="editMemberBaseInfo" class="com.gzjky.action.healthRecordAction.EditMemberBaseInfoAction" method="editBaseInfo">
     	<result name="success">/jsp/health/healthrecord/memberbaseinfo.jsp</result>
	</action>
	
	<!-- 基本信息-详细信息编辑 -->
	<action name="editMemberDetail" class="com.gzjky.action.healthRecordAction.EditMemberDetailAction" method="editDetailInfo">
     	<result name="success">/jsp/health/healthrecord/memberbaseinfo.jsp</result>
	</action>
	
	<!-- 基本信息-详细信息编辑 -->
	<action name="editMemberWorkInfo" class="com.gzjky.action.healthRecordAction.EditMemberWorkInfoAction" method="editWorkInfo">
     	<result name="success">/jsp/health/healthrecord/memberbaseinfo.jsp</result>
	</action>
	
	<!-- 患者生活习惯-->
	<action name="queryMemberHabit" class="com.gzjky.action.healthRecordAction.QueryMemberHabitAction" method="getHabitInfo">
     	<result name="success">/jsp/health/healthrecord/memberhabit.jsp</result>
	</action>
	
	<!-- 患者生活习惯更新-->
	<action name="editMemberHabit" class="com.gzjky.action.healthRecordAction.EditMemberHabitAction" method="editHabitInfo">
     	<result name="success">/jsp/health/healthrecord/memberhabit.jsp</result>
	</action>
	
	<!-- 患者当前并发症更新-->
	<action name="editMemberHtComplication" class="com.gzjky.action.healthRecordAction.EditMemberHtComplicationAction" method="editComplicationInfo">
     	<result name="success">/jsp/health/healthrecord/memberhtcomplication.jsp</result>
	</action>
	
	<!-- 疾病史查询-->
	<action name="queryMemberIllnessHistoryList" class="com.gzjky.action.healthRecordAction.QueryMemberIllnessHistoryListAction" method="getList">
     	<result name="success">/jsp/health/healthrecord/memberIllnessHistory.jsp</result>
	</action>
	

	</package>
</struts>
